# ~/.ssh/ssh_config

Host localhost
    NoHostAuthenticationForLocalhost yes
    Compression no

Host thorsten t
    Hostname v2202301190876213924.hotsrv.de

Host git
    Hostname git.zitc.de
    User git

Host webserver
    Hostname 10.129.7.9
    ProxyCommand ssh -W %h:%p thorsten
    User root

Host resilio
    Hostname 10.129.7.204
    ProxyCommand ssh -W %h:%p thorsten
    LocalForward 8888 localhost:8888
    User root

Host pik
    Hostname aix.pik-potsdam.de

Host se62 se94 se121 se157 se152
    Hostname %h.pik-potsdam.de
    ProxyCommand ssh -W %h:%p pik
    LocalForward 8080 localhost:8080

Host *.zeit.de
    User witsch
    StrictHostKeyChecking no

Host *
    PasswordAuthentication no
    ForwardAgent yes
    Compression yes
    ServerAliveInterval 60
